############
# Secrets
# YOU MUST CHANGE THESE BEFORE GOING INTO PRODUCTION
############

# @meta {"description":"Postgres-Passwort für den internen postgres-User","scopes":["dev","prod"],"defaults":{"dev":"change-me","prod":"change-me"}}
POSTGRES_PASSWORD=change-me
# @meta {"description":"JWT-Secret für Supabase Auth Tokens","scopes":["dev","prod"],"defaults":{"dev":"your-jwt-secret","prod":"your-jwt-secret"}}
JWT_SECRET=your-jwt-secret-change-me
# @meta {"description":"Anon-Key für öffentliche Supabase-Clients","scopes":["dev","prod"],"defaults":{"dev":"replace-with-anon-key","prod":"replace-with-anon-key"}}
ANON_KEY=replace-with-anon-key
# @meta {"description":"Service-Role-Key mit erhöhten Rechten","scopes":["dev","prod"],"defaults":{"dev":"replace-with-service-role-key","prod":"replace-with-service-role-key"}}
SERVICE_ROLE_KEY=replace-with-service-role-key
# @meta {"description":"Benutzername für das Supabase Dashboard","scopes":["dev","prod"],"defaults":{"dev":"supabase-admin","prod":"supabase-admin"}}
DASHBOARD_USERNAME=supabase-admin
# @meta {"description":"Passwort für das Supabase Dashboard","scopes":["dev","prod"],"defaults":{"dev":"change-me","prod":"change-me"}}
DASHBOARD_PASSWORD=change-this-password
# @meta {"description":"Secret Key Base für Supabase Studio","scopes":["dev","prod"],"defaults":{"dev":"replace-with-secret-key-base","prod":"replace-with-secret-key-base"}}
SECRET_KEY_BASE=replace-with-secret-key-base
# @meta {"description":"Verschlüsselungs-Key für Vault","scopes":["dev","prod"],"defaults":{"dev":"replace-with-encryption-key","prod":"replace-with-encryption-key"}}
VAULT_ENC_KEY=replace-with-encryption-key-32-chars
# @meta {"description":"Verschlüsselungs-Key für pg-meta","scopes":["dev","prod"],"defaults":{"dev":"replace-with-encryption-key","prod":"replace-with-encryption-key"}}
PG_META_CRYPTO_KEY=replace-with-encryption-key-32-chars


############
# Database - You can change these to any PostgreSQL database that has logical replication enabled.
############

# @meta {"description":"Hostname für die Postgres-Instanz","scopes":["dev","prod"],"defaults":{"dev":"db","prod":"db"}}
POSTGRES_HOST=db
# @meta {"description":"Name der Standarddatenbank","scopes":["dev","prod"],"defaults":{"dev":"app","prod":"aiTestProdDB"}}
POSTGRES_DB=aiTestProdDB
# @meta {"description":"Externer Port für Postgres-Verbindungen","scopes":["dev","prod"],"defaults":{"dev":"54322","prod":"54322"}}
POSTGRES_PORT=54322
# default user is postgres


############
# Supavisor -- Database pooler
############

# @meta {"description":"Port für Supavisor Transaction Pooling","scopes":["dev","prod"],"defaults":{"dev":"6543","prod":"6543"}}
POOLER_PROXY_PORT_TRANSACTION=6543
# @meta {"description":"Standard-Poolgröße pro Datenbank","scopes":["dev","prod"],"defaults":{"dev":"20","prod":"20"}}
POOLER_DEFAULT_POOL_SIZE=20
# @meta {"description":"Maximale Client-Verbindungen pro Pool","scopes":["dev","prod"],"defaults":{"dev":"100","prod":"100"}}
POOLER_MAX_CLIENT_CONN=100
# @meta {"description":"Tenant-ID für Supavisor","scopes":["dev","prod"],"defaults":{"dev":"ai-test","prod":"ai-test"}}
POOLER_TENANT_ID=ai-test
# @meta {"description":"Poolgröße für interne Metadaten","scopes":["dev","prod"],"defaults":{"dev":"5","prod":"5"}}
POOLER_DB_POOL_SIZE=5


############
# API Proxy - Configuration for the Kong Reverse proxy.
############

# @meta {"description":"HTTP-Port des Kong-Proxys","scopes":["dev","prod"],"defaults":{"dev":"8000","prod":"8000"}}
KONG_HTTP_PORT=8000
# @meta {"description":"HTTPS-Port des Kong-Proxys","scopes":["dev","prod"],"defaults":{"dev":"8443","prod":"8443"}}
KONG_HTTPS_PORT=8443


############
# API - Configuration for PostgREST.
############

# @meta {"description":"Schemas, die PostgREST bereitstellt","scopes":["dev","prod"],"defaults":{"dev":"public,storage,graphql_public","prod":"public,storage,graphql_public"}}
PGRST_DB_SCHEMAS=public,storage,graphql_public


############
# Auth - Configuration for the GoTrue authentication server.
############

## General
# @meta {"description":"Öffentliche Basis-URL der Anwendung","scopes":["dev","prod"],"defaults":{"dev":"http://localhost:3000","prod":"https://ai-test.dakatos.online"}}
SITE_URL=https://ai-test.dakatos.online
# @meta {"description":"Zusätzliche Redirect-URLs (Komma-separiert)","scopes":["dev","prod"],"defaults":{"dev":"","prod":""}}
ADDITIONAL_REDIRECT_URLS=
# @meta {"description":"Gültigkeitsdauer der JWTs in Sekunden","scopes":["dev","prod"],"defaults":{"dev":"3600","prod":"3600"}}
JWT_EXPIRY=3600
# @meta {"description":"Registrierung komplett deaktivieren","scopes":["dev","prod"],"defaults":{"dev":"false","prod":"false"}}
DISABLE_SIGNUP=false
# @meta {"description":"Externe URL für Supabase API","scopes":["dev","prod"],"defaults":{"dev":"http://localhost:8000","prod":"https://sb-ai-test.dakatos.online"}}
API_EXTERNAL_URL=https://sb-ai-test.dakatos.online

## Mailer Config
# @meta {"description":"Pfad für Bestätigungslinks","scopes":["dev","prod"],"defaults":{"dev":"/auth/v1/verify","prod":"/auth/v1/verify"}}
MAILER_URLPATHS_CONFIRMATION="/auth/v1/verify"
# @meta {"description":"Pfad für Einladungslinks","scopes":["dev","prod"],"defaults":{"dev":"/auth/v1/verify","prod":"/auth/v1/verify"}}
MAILER_URLPATHS_INVITE="/auth/v1/verify"
# @meta {"description":"Pfad für Recovery-Links","scopes":["dev","prod"],"defaults":{"dev":"/auth/v1/verify","prod":"/auth/v1/verify"}}
MAILER_URLPATHS_RECOVERY="/auth/v1/verify"
# @meta {"description":"Pfad für E-Mail-Änderungslinks","scopes":["dev","prod"],"defaults":{"dev":"/auth/v1/verify","prod":"/auth/v1/verify"}}
MAILER_URLPATHS_EMAIL_CHANGE="/auth/v1/verify"

## Email auth
# @meta {"description":"Erlaubt E-Mail-Signups","scopes":["dev","prod"],"defaults":{"dev":"true","prod":"true"}}
ENABLE_EMAIL_SIGNUP=true
# @meta {"description":"Automatische Bestätigung per E-Mail aktivieren","scopes":["dev","prod"],"defaults":{"dev":"false","prod":"false"}}
ENABLE_EMAIL_AUTOCONFIRM=false
# @meta {"description":"Absenderadresse für Admin-Mails","scopes":["dev","prod"],"defaults":{"dev":"admin@example.com","prod":"admin@example.com"}}
SMTP_ADMIN_EMAIL=admin@example.com
# @meta {"description":"SMTP-Host für Auth-E-Mails","scopes":["dev","prod"],"defaults":{"dev":"mailpit","prod":"smtp.example.com"}}
SMTP_HOST=mailpit
# @meta {"description":"SMTP-Port","scopes":["dev","prod"],"defaults":{"dev":"1025","prod":"587"}}
SMTP_PORT=1025
# @meta {"description":"SMTP-Benutzername","scopes":["dev","prod"],"defaults":{"dev":"","prod":"smtp-user"}}
SMTP_USER=
# @meta {"description":"SMTP-Passwort","scopes":["dev","prod"],"defaults":{"dev":"","prod":"smtp-pass"}}
SMTP_PASS=
# @meta {"description":"SMTP-Absendername","scopes":["dev","prod"],"defaults":{"dev":"\"Supabase Local\"","prod":"\"Supabase\""}}
SMTP_SENDER_NAME="Supabase Local"
# @meta {"description":"Anonyme Nutzer erlauben","scopes":["dev","prod"],"defaults":{"dev":"false","prod":"false"}}
ENABLE_ANONYMOUS_USERS=false

## Phone auth
# @meta {"description":"Telefon-Registrierung erlauben","scopes":["dev","prod"],"defaults":{"dev":"true","prod":"true"}}
ENABLE_PHONE_SIGNUP=true
# @meta {"description":"Telefonnummern automatisch bestätigen","scopes":["dev","prod"],"defaults":{"dev":"true","prod":"true"}}
ENABLE_PHONE_AUTOCONFIRM=true


